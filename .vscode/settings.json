{
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/assets/css",
      "savePathReplacementPairs": null,
      "generateMap": true
    },
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/assets/css",
      "savePathReplacementPairs": null,
      "generateMap": true
    }
  ],
  "[html]": {
    "editor.tabSize": 4
  },
  "editor.detectIndentation": false
}
